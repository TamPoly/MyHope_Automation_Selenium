package basic;

public class toan_tu_2 {
	public static void main(String[] args) {
		int a = 5;
		int b = 10;
		int c = 15;
		int d = 20;
		int e = 25;
		int f = 30;
		
		//b += a <=> b = b + a
				
		System.out.println(b += a);
		System.out.println(c -= a);
		System.out.println(d *= a);
		System.out.println(e /= a);
		System.out.println(f >>= a);

	}
	
}
