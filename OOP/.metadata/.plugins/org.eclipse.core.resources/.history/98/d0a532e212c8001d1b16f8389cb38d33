package basic;

public class toan_tu {
	
	int ten = 10;
	int five = 5;
	
	double four = 4.5;
	double two = 2.5;
	
	float chieu_dai = 20;
	float chieu_rong = 10;
	
	String first_name = "Tâm";
	String last_name = "Đinh";
	
	public static void main(String[] args) {
		
		//Cộng, Trừ, Nhân, Chia
		System.out.println("Tổng giữ 2 số là: " + (ten+five));
		
	}
}
