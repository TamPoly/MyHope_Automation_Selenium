package OOP;

import java.util.Scanner;

public class Student {
	String name;
	int age;

	public void cons(String name, int age) {
		this.name = name;
		this.age = age;
	}

	public void getInfo(String name, int age) {
		System.out.println("My name: " + name);
		System.out.println("My age: " + age);
	}
}
