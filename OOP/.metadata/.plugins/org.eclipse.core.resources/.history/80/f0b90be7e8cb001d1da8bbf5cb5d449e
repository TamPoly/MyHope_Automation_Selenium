package basic;

public class convert {

	public static void main(String[] args) {
		// String to Number
		String str1 = "10";
		String str2 = "20";
		
		int in1 = Integer.valueOf(str1).intValue();
		int in2 = Integer.valueOf(str2).intValue();
		
		// Cách viết khác
		
		double db1 = Double.parseDouble(str1);
		double db2 = Double.parseDouble(str2);
		

	}

}
